{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "npm run fix:prettier",
    "fix:prettier": "prettier --write .",
    "check:prettier": "prettier --check .",
    "check:types": "tsc --noEmit",
    "check": "concurrently npm:check:*"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.8.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@types/node": "^22.13.8",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "prettier": "3.5.3",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-imagetools": "^7.0.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom"
      ]
    },
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  },
  "packageManager": "pnpm@9.15.2"
}
